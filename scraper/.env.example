# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Proxy Configuration (optional)
# Comma-separated list of proxies for rotation
# Supports HTTP, HTTPS, SOCKS5
# PROXY_LIST=http://user:pass@proxy1:8080,socks5://user:pass@proxy2:1080
PROXY_LIST=

# Scraper Configuration
# Maximum concurrent requests per domain
MAX_CONCURRENT_PER_DOMAIN=2

# Minimum delay between requests in ms
MIN_REQUEST_DELAY=1500

# Maximum delay between requests in ms
MAX_REQUEST_DELAY=4500

# Enable/disable specific scrapers (true/false)
ENABLE_LEVER=true
ENABLE_GREENHOUSE=true
ENABLE_ASHBY=true
ENABLE_WELLFOUND=true
ENABLE_NAUKRI=true
ENABLE_LINKEDIN=true
ENABLE_INDEED=true

# Logging level (error, warn, info, debug)
LOG_LEVEL=info

# Session rotation interval in minutes
SESSION_ROTATION_INTERVAL=15

# Maximum retries per request
MAX_RETRIES=3
